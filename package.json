{
  "name": "angular1-webpack4",
  "version": "1.0.0",
  "description": "Using webpack 4with an angularJS 1 project",
  "main": "app/main.js",
  "repository": "",
  "scripts": {
    "start": "webpack-dev-server --mode development --progress",
    "dev": "webpack --mode development",
    "test": "jest --coverage",
    "dev:watch": "webpack --mode development --watch",
    "build": "webpack --mode production"
  },
  "keywords": [
    "webpack",
    "angular"
  ],
  "author": "PHY",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "7.2.3",
    "angular-formly": "^4.0.8",
    "autoprefixer": "9.4.4",
    "babel-jest": "^24.0.0",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "1.0.0",
    "css-loader": "2.1.0",
    "eslint": "5.12.0",
    "eslint-loader": "2.1.1",
    "file-loader": "3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "3.2.0",
    "jest": "^24.0.0",
    "jest-cli": "^24.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "postcss-loader": "3.0.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "url-loader": "^2.1.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "angular": "^1.3.14",
    "angular-animate": "^1.3.14",
    "angular-ui-router": "^0.2.13"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "./app/js/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 50,
        "lines": 33,
        "statements": 25
      }
    }
  }
}
